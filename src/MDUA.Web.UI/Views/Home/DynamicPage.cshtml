@model string
@{
    ViewData["Title"] = ViewBag.PageTitle;
    Layout = "_Layout";
}

<div class="container my-5">
    <div class="row justify-content-center">
        <div class="col-lg-10">
            <h1 class="mb-4 fw-bold">@ViewBag.PageTitle</h1>
            <div class="bg-white p-5 shadow-sm rounded">
                @Html.Raw(Model)
            </div>
        </div>
    </div>
</div>